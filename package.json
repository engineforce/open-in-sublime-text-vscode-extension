{
  "name": "open-in-sublime-text",
  "displayName": "open in sublime text",
  "description": "open folder in sublime text",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:open-in-sublime-text.openInSublimeText"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "open-in-sublime-text.openInSublimeText",
        "title": "Open in Sublime Text"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "open-in-sublime-text.openInSublimeText"
        }
      ],
      "editor/context": [
        {
          "command": "open-in-sublime-text.openInSublimeText"
        }
      ],
      "editor/title": [
        {
          "command": "open-in-sublime-text.openInSublimeText"
        }
      ],
      "editor/title/context": [
        {
          "command": "open-in-sublime-text.openInSublimeText"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "exec-sh": "^0.4.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  }
}
